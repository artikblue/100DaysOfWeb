#!/home/lab/100DaysOfWeb/9-12-apistar/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'apistar==0.5.42','console_scripts','apistar'
__requires__ = 'apistar==0.5.42'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('apistar==0.5.42', 'console_scripts', 'apistar')()
    )
